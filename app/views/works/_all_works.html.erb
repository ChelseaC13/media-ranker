<table class="table margin">
  <tr>
    <th>Votes</th>
    <th>Title</th>
    <th>Created By</th>
    <th>Published</th>
    <th>Upvote</th>
  </tr>

  <% work_group = Work.order("#{action_name}")%>
    <% work_group.each do |work| %>
      <tr>
        <td><%= Work.vote_total(work.id) %></td>
        <td><%= link_to work.title, work_path(work.id) %></td>
        <td><%= work.creator %></td>
        <td><%= work.publication_year %></td>
        <td class="btn btn-primary "><%= link_to "Upvote", work_votes_path(work.id), method: :post%></td> 
      </tr>
    <% end %>
</table>
